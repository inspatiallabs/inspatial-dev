{
  "name": "@inspatial/run",
  "license": "Intentional-License-1.0",
  "version": "0.0.1",
  "imports": {
    "@inspatial/run": "./src/index.ts",
    "@inspatial/run/jsx": "./src/jsx/index.ts",
    "@inspatial/run/hmr": "./src/hmr/index.ts",
    "@rollup/pluginutils": "npm:@rollup/pluginutils@^5.2.0"
  },
  "exports": {
    ".": "./src/index.ts",
    "./jsx": "./src/runtime/jsx/index.ts",
    "./hmr": "./src/hmr/index.ts"
  },
  "tasks": {
    "test": "deno test --allow-env --allow-read --allow-write --allow-run --allow-net --allow-hrtime --allow-env --allow-all ./src/index.test.ts"
  },
  "publish": {
    "exclude": [".github", ".vscode", "**/*.test.ts", "test_deps.ts"]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@inspatial/run",
    "lib": ["dom", "dom.iterable", "esnext"]
  }
}
