{
  "name": "@inspatial/renderer",
  "license": "Intentional-License-1.0",
  "version": "0.1.0",
  "description": "InSpatial Universal Renderer (GPU, DOM, Native)",
  "imports": {},
  "exports": {
    ".": "./src/index.ts"
  },
  "tasks": {
    "test": "deno test --allow-env --allow-read --allow-write --allow-run --allow-net --allow-hrtime --allow-all ./src/**/*.test.ts"
  },
  "compilerOptions": {
    "checkJs": false,
    "jsx": "react",
    "jsxFactory": "createElement",
    "jsxFragmentFactory": "Fragment",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": false
  },
  "publish": {
    "exclude": [".github", ".vscode", "**/*.test.ts", "test_deps.ts"]
  }
}
